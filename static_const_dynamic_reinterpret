#include <iostream>

class A{
    public :
    virtual void f()=0;
};

class B : public A{
    public :
    void f() override {
        std::cout << "B";
    }
};

class C : public A{
    public :
    void f() override {
        std::cout << "C";
    }
};


// const_cast modify value here
int f(int *x){
    return (*x+20);
}


int main()
{
  // static_cast
  double x = 10.05;
  int y = static_cast <int> (x);
  std::cout<<y <<std::endl;
  
  // const_cast
  const int val = 20;
  int *val_t = const_cast <int*> (&val);
  int xy = f(val_t);
  std::cout<<*val_t << xy <<std::endl;
  
  //dynamic_cast
  A * a = new B();
  B * b = dynamic_cast <B*> (a);
  b->f();

  // reinterpret_cast
  int z =10;
  char *ch = reinterpret_cast <char*> (&z);
  std::cout<<ch;

  return 0;
}

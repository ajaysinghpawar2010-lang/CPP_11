#include <iostream>

void f(int *p){
    std::cout<<"int ptr called"<<std::endl;
}

void f(char *p){
    std::cout<<"char ptr called"<<std::endl;
}


int main()
{
    int *ip= NULL;
    char *c = nullptr;
    f(ip);
    f(c);
    return 0;
}

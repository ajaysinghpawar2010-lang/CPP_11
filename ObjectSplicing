#include <iostream>
class A{
    public : 
    void f(){
        std::cout << "A" << std::endl;
    }
    
};



class B : public A {
    public : 
    void h(){
        std::cout << "B" << std::endl;
    }
    
};


int main()
{
    B b;
    b.f();
    b.h();
    A a(b);
    a.f();  // a.h() cannot be called since object is spliced here 

    
    return 0;
}

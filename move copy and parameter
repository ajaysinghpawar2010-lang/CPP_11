#include <iostream>

// casting correct

class A {
    int *x;
public :
    A (int val) : x(new int(val)){
        std::cout << "Parameterized cintructior" << std::endl;
    }
    A (const A &a){
        x = new int (*a.x);
        std::cout << "copy cintructior" << std::endl;
    }
	
	A(A &&a) noexcept{
	    x = a.x;
	    a.x = nullptr;
	    std::cout << "move cintructior" << std::endl;
	}
	
};


int main()
{
    A * a = new A(5);
    A * b = new A(*a);
    A b1(std::move(*a));
	return 0;
}
